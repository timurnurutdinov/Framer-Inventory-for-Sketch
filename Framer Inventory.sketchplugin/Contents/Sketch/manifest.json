{
  "name" : "Framer Inventory",
  "author" : "Timur Nurutdinov",
  "authorEmail" : "framerinventory@gmail.com",
  "description": "Import States & Export Prototypes from Sketch to Framer",
  "homepage": "https://timurnurutdinov.github.io/framerinventory/?utm_source=plugin&utm_medium=manifest",
  "identifier": "com.addleimb.framer-inventory",
  "version": "3.0.1",
  "appcast": "https://raw.githubusercontent.com/timurnurutdinov/tree/sketch-update-tool/Framer-Inventory-for-Sketch/plugin-appcast.xml",
  "compatibleVersion": 4.3,
  "commands" : [
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onReplicateLayersRun",
      "shortcut" : "",
      "name" : "⊡   Import Layers",
      "identifier" : "choose-replicate-layers",
      "description" : "Generate Framer layers",
      "icon" : "layers.png"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onGenerateStatesRun",
      "shortcut" : "",
      "name" : "⧉   Import States",
      "identifier" : "choose-generate-states",
      "description" : "Generate Framer States",
      "icon" : "states.png"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onSimulateKeynoteRun",
      "shortcut" : "",
      "name" : "❖   Export Prototype",
      "identifier" : "choose-simulate-keynote",
      "description" : "Export Framer Prototype",
      "icon" : "scene.png"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onTestingRun",
      "shortcut" : "",
      "name" : "Run testing environment",
      "identifier" : "choose-testing-environment"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onSettingRun",
      "shortcut" : "",
      "name" : "      Settings",
      "identifier" : "choose-settings"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onRemoveExportPageRun",
      "shortcut" : "",
      "name" : "Remove Export Page",
      "identifier" : "choose-remove-export-page"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onShowBarRun",
      "shortcut" : "",
      "name" : "      Show Toolbar",
      "identifier" : "show-bar"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onSelectProjectFolderRun",
      "shortcut" : "",
      "name" : "Select Framer Folder",
      "identifier" : "choose-select-export-path"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onRemoveProjectFolderRun",
      "shortcut" : "",
      "name" : "Remove Framer Folder",
      "identifier" : "choose-remove-export-path"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onShowProjectFolderRun",
      "shortcut" : "",
      "name" : "Show Framer Folder",
      "identifier" : "choose-show-export-path"
    },
    {
      "script" : "run-actions.cocoascript",
      "handler" : "onCheckUpdatesRun",
      "shortcut" : "",
      "name" : "Check for Updates",
      "identifier" : "choose-check-updates"
    },


    {
      "script" : "selection-changed-action.cocoascript",
      "handlers" : {
        "actions" : {
           "SelectionChanged.finish" : "onSelectionChanged"
           }
          }
    }
  ],
  "menu": {
	  "title": "Framer Inventory",
      "items": [
        "show-bar",
        "-",
  		  "choose-replicate-layers",
        "choose-generate-states",
        "-",
        "choose-simulate-keynote",
        "-",
        {
          "title": "      Framer Folder",
          "items": [
            "choose-show-export-path",
            "-",
            "choose-select-export-path",
            "choose-remove-export-path"
          ]
        },
  		  {
  		         "title": "      Utils",
  		         "items": [
  					          "choose-remove-export-page",
                      "choose-check-updates"
  		          ]
  		  },
        "-",
        "choose-settings"
      ]
    }
}
